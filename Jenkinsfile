  agent any
  stages {
    stage('code-build') {
      steps {
        echo 'this is my build steps '
      }
    }

    stage('unit-test') {
      steps {
        echo 'this is my test'
      }
    }

    stage('deploy') {
      steps {
        echo 'this is my deploy stage'
      }
    }

  }
}